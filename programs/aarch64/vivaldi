#!/bin/sh

echo ""
echo " Vivaldi browser is provided by https://vivaldi.com"
echo ""
echo " 1) Vivaldi Stable, the public release (RECOMMENDED)"
echo ""
echo " 2) Vivaldi Snapshot, the testing version (may be a bit unstable)"
echo ""
read -r -p " What do you choose between 1 and 2? " response
case "$response" in
    1) APP=vivaldi-stable

# CREATING THE FOLDER
mkdir /opt/$APP
cd /opt/$APP;

# ADD THE REMOVER
echo '#!/bin/sh' >> /opt/$APP/remove
echo "rm -R -f /usr/local/share/applications/$APP-AM.desktop /opt/$APP /usr/local/bin/$APP" >> /opt/$APP/remove
chmod a+x /opt/$APP/remove

mkdir tmp;
cd tmp;

# DOWNLOADING THE DEPENDENCIES
version=$(curl -s https://archlinux.org/packages/community/x86_64/vivaldi/ | grep -Eo "vivaldi [+-]?[0-9]+([.][0-9]+)?+[.]?[0-9]" | head -1 | cut -c 9-)
wget https://downloads.vivaldi.com/stable/vivaldi-stable_$version-1_arm64.deb
ar x ./*deb
tar fx ./data.tar.xz
cd ..
echo $version >> ./version
mv ./tmp/opt/vivaldi/* ./
rm -R -f ./tmp

# LINK
ln -s /opt/$APP/vivaldi /usr/local/bin/$APP

# SCRIPT TO UPDATE THE PROGRAM
echo '#!/usr/bin/env bash
version0=$(cat /opt/'$APP'/version)
version=$(curl -s https://archlinux.org/packages/community/x86_64/vivaldi/ | grep -Eo "vivaldi [+-]?[0-9]+([.][0-9]+)?+[.]?[0-9]" | head -1 | cut -c 9-)
if [[ "$version0" == "$version" ]]; then
  echo "Update not needed, exit!"
 else
  notify-send "A new version of '$APP' is available, please wait!"
  mkdir /opt/'$APP'/tmp
  cd /opt/'$APP'/tmp
  wget https://downloads.vivaldi.com/stable/vivaldi-stable_$version-1_arm64.deb
  ar x ./*deb
  tar fx ./data.tar.xz
  cd ..
  rm -R -f ./version
  mv --backup=t ./tmp/opt/vivaldi/* ./
  echo $version >> ./version
  rm -R -f ./tmp ./*~
fi' >> /opt/$APP/AM-updater
chmod a+x /opt/$APP/AM-updater

# LAUNCHER
echo "[Desktop Entry]
Version=1.0
Name=Vivaldi
# Only KDE 4 seems to use GenericName, so we reuse the KDE strings.
# From Ubuntu's language-pack-kde-XX-base packages, version 9.04-20090413.
GenericName=Web Browser
GenericName[ar]=متصفح الشبكة
GenericName[bg]=Уеб браузър
