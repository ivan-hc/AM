mkdir tmp;
cd tmp;

# DOWNLOADING THE DEPENDENCIES
version=$(curl -s https://aur.archlinux.org/packages/vivaldi-snapshot | grep -Eo "vivaldi-snapshot [+-]?[0-9]+([.][0-9]+)?+[.]?[0-9]" | head -1 | cut -c 18-)
wget https://downloads.vivaldi.com/snapshot/vivaldi-snapshot_$version-1_arm64.deb
ar x ./*deb
tar fx ./data.tar.xz
